    "moment": "2.22.2",
    "node-sass-chokidar": "1.3.0",
    "npm-run-all": "4.1.3",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "react": "^16.4.1",
    "react-datetime": "2.14.0",
    "react-dom": "^16.4.1",
    "react-i18next": "^7.8.0",
    "react-nouislider": "2.0.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.4",
    "react-slick": "0.23.1",
    "react-spinners": "^0.3.3",
    "react-swipeable-views": "0.12.13",
